{
	"schemaVersion": 1,
	"id": "mcoaster_waterpark",
	"version": "${version}",
	"name": "MCoaster Waterpark",
	"description": "Waterpark-themed fork of MCoaster (originally Splinecart), with separate mod IDs for compatibility.",
	"authors": [
		"SuperMatty46"
	],
	"contact": {
		"homepage": "https://github.com/SuperMatty46/MCoaster-Waterpark",
		"sources": "https://github.com/SuperMatty46/MCoaster-Waterpark"
	},
	"license": "MIT",
	"icon": "assets/splinecart/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.foundationgames.splinecart.Splinecart"
		],
		"client": [
			"io.github.foundationgames.splinecart.SplinecartClient"
		],
		"modmenu": [
			"io.github.foundationgames.splinecart.config.ModMenuIntegration"
		]
	},
	"mixins": [
		"splinecart.mixins.json",
		{
			"config": "splinecart.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"yet_another_config_lib_v3": ">=3.8.0+1.21.4-fabric"
	}
}
